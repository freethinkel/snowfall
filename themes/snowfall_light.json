    "editorBracketHighlight.foreground5": "#50afce",
    "editorBracketHighlight.foreground6": "#d59225"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#bdc1c8"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a25cb5"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#d59225"
      }
    },
    {
      "scope": "storage.type.primitive",
      "settings": {
        "foreground": "#a25cb5"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#85B300"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d59225"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.function"
      ],
      "settings": {
        "foreground": "#50afce"
      }
    },
    {
      "scope": "markup.heading.markdown",
      "settings": {
        "foreground": "#50afce",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#d59225"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#85B300"
      }
    },
    {
      "scope": "markup.list punctuation.definition.list.begin",
      "settings": {
        "foreground": "#d59225"
      }
    },
    {
      "scope": "storage.type.annotation",
      "settings": {
        "foreground": "#d59225"
      }
    },
    {
      "scope": [
        "meta.tag.attributes",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#50afce"
      }
    }
  ]
}