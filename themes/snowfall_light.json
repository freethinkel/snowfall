    "editorBracketHighlight.foreground6": "#93b8c5"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#474c54"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B08CBA"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#EBD2A7"
      }
    },
    {
      "scope": "storage.type.primitive",
      "settings": {
        "foreground": "#B08CBA"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BDB969"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#EBD2A7"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.function"
      ],
      "settings": {
        "foreground": "#7FB2C7"
      }
    },
    {
      "scope": "storage.type.annotation",
      "settings": {
        "foreground": "#EBD2A7"
      }
    }
  ]
}